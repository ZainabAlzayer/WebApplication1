
@{
    ViewBag.Title = "ComputerIndex";
}

<!DOCTYPE html>
<html>
<head>
    <style>
        table, th, td {
            border: 1px solid black;
            border-collapse: collapse;
        }

        th, td {
            padding: 5px;
            text-align: left;
        }
    </style>
</head>
<body>




    <table style="width:100%">

        @foreach (var Equipments in Model)
        {
            <tr>
                <th>ID</th>
                <td>@Equipments.ID</td>
            </tr>
                <tr>
                    <th>Code</th>

                    <td>@Equipments.Code</td>
                </tr>

                    <tr>
                        <th>Name</th>

                        <td>@Equipments.Name</td>
                    </tr>
                    <tr>
                        <th>Description </th>

                        <td>@Equipments.Description </td>

                    </tr>

        }
    </table>
    <a href="@Url.Action("Index","Inventory")" class="elements"> Back to Inventory </a>


</body>
</html>
